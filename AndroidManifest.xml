<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="edu.tcnj.spamblocker"
      android:versionCode="1"
      android:versionName="1.0">
      <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17" />
      <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS"/>
      <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
      <uses-permission android:name="android.permission.MODIFY_PHONE_STATE"/>
      <uses-permission android:name="android.permission.CALL_PHONE"/>
      <uses-permission android:name="android.permission.READ_CONTACTS"/>
      <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:icon="@drawable/ic_launcher" android:allowBackup="true" android:label="@string/app_name">
        <activity android:name="edu.tcnj.spamblocker.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
               
                <receiver android:name="edu.tcnj.spamblocker.IncomingCall">
                        <intent-filter>
                            <action android:name="android.intent.action.PHONE_STATE"/>
                        </intent-filter>
                </receiver>
               
    </application>



</manifest> 